{
  "name": "@tokene/ui-kit",
  "version": "0.0.0-rc.2",
  "description": "Collection of common TokenE vue components",
  "repository": {
    "type": "git",
    "url": "https://github.com/dl-tokene/webkit/tree/main/packages/ui-kit"
  },
  "homepage": "https://dl-tokene.github.io/webkit/modules/_tokene_ui-kit.html",
  "license": "MIT",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "typesVersions": {
    ">=4.2": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/es/index.js",
  "browser": {
    "./sfc": "./dist/es/index.js"
  },
  "unpkg": "./dist/index.js",
  "types": "dist/types/index.d.ts",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/index.js"
    },
    "./src": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./dist/index.css": {
      "default": "./dist/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && vite build && node ./postbuild.js && yarn build:web-types",
    "build:web-types": "vue-docgen-web-types --config web-types-gen.config.json",
    "clean": "rm -rf dist",
    "test": "yarn jest --verbose --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@distributedlab/fetcher": "0.2.0-rc.6",
    "@distributedlab/jac": "0.2.0-rc.6",
    "@distributedlab/tools": "0.2.0-rc.6",
    "@distributedlab/w3p": "0.2.0-rc.6",
    "@tokene/styles": "workspace:^",
    "@tokene/toolkit": "workspace:^",
    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "@vueuse/core": "^10.1.0",
    "axios": "^1.3.6",
    "i18next": "^22.4.15",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.1",
    "vue-router": "^4.1.6"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.1",
    "@types/lodash-es": "^4",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.2.1",
    "jest": "^29.5.0",
    "rollup": "^3.18.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-vue": "^6.0.0",
    "tsc-alias": "^1.8.2",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.3",
    "vite-plugin-dts": "^2.3.0",
    "vue-docgen-web-types": "^0.1.8",
    "vue-tsc": "^1.6.1"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "@tokene/ui-kit"
  },
  "web-types": "./web-types.json"
}
